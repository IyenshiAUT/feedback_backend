{
  "name": "team-mimh-feedback-backend",
  "version": "1.0.0",
  "description": "Backend API for Team MIMH Project Feedback System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "deploy": "wrangler deploy",
    "dev:worker": "wrangler dev",
    "d1:create": "wrangler d1 create team-mimh-feedback",
    "d1:init": "wrangler d1 execute team-mimh-feedback --file=./schema.sql",
    "d1:init:local": "wrangler d1 execute team-mimh-feedback --local --file=./schema.sql"
  },
  "dependencies": {
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "wrangler": "^3.22.0"
  }
}
